{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "smartq (debug mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "debug"
    },
    {
      "name": "smartq (profile mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile"
    },
    {
      "name": "smartq (release mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release"
    },
    {
      "name": "Flutter Web (Debug)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "debug",
      //4000 to 4003 multiple instance support
      //update --web-renderer base on your project
      "args": ["--web-port=4000", "--web-renderer=canvaskit"] 
    },
    {
      "name": "Launch Current Opened File",
      "type": "node",
      "request": "launch",
      "restart": true,
      "runtimeExecutable": "nodemon",
      "program": "path to project /proxy-server.js",
      "console": "integratedTerminal"
    }
  ]
}
